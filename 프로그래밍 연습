#include <iostream>

using namespace std;
int n;
int x[20];
int y[20];

int main() {
    cin >> n;
    for(int i=0; i<n; i++){
        cin >> x[i] >> y[i];
    }

    int ans = 0;
    
    for(int i=0; i<=10; i++){
        for(int j=0; j<=10; j++){
            for(int k=0; k<=10; k++){
                
                //x축에 평행한 직선 3개
                bool flag = true;
                for(int w=0; w<n; w++){
                    if(x[w] == i || x[w] == j || x[w] == k){
                        continue;
                    }
                    flag = false;
                }
                if(flag) ans = 1;
                
                //x축에 평행한 직선 1개, y축에 평행한 직선 2개
                flag = true;
                for(int w=0; w<n; w++){
                    if(x[w] == i || y[w] == j || y[w] == k){
                        continue;
                    }
                    flag = false;
                }
                if(flag) ans = 1;

                //x축에 평행한 직선 2개, y축에 평행한 직선 1개
                flag = true;
                for(int w=0; w<n; w++){
                    if(x[w] == i || x[w] == j || y[w] == k){
                        continue;
                    }
                    flag = false;
                }
                if(flag) ans = 1;

                //y축에 평행한 직선 3개
                flag = true;
                for(int w=0; w<n; w++){
                    if(y[w] == i || y[w] == j || y[w] == k){
                        continue;
                    }
                    flag = false;
                }
                if(flag) ans = 1;
            }
        }
    }
    cout << ans;

    return 0;
}
